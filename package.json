{
  "name": "persistenceonejs",
  "version": "1.0.4",
  "description": "Client side JS libraries for persistenceSDK transaction generation, signing and broadcasting.",
  "main": "main/index.js",
  "module": "module/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "main",
    "module",
    "types"
  ],
  "scripts": {
    "test-gov-contract": "NODE_ENV=development ts-node ./examples/ContractProposals.ts",
    "build:main": "BABEL_ENV=production babel src --out-dir main --delete-dir-on-start --extensions \".tsx,.ts,.js\"",
    "build:module": "MODULE=true babel src --out-dir module --delete-dir-on-start  --extensions \".tsx,.ts,.js\"",
    "build": "npm run build:module && npm run build:main",
    "build:ts-proto": "NODE_ENV=development npm run clean && ./scripts/ts-proto-gen.sh && cti create ./src -w",
    "build:telescope": "NODE_ENV=development ts-node ./scripts/telescope.ts",
    "build:contracts": "NODE_ENV=development ts-node ./scripts/contracts.ts",
    "clean": "NODE_ENV=development ./scripts/prepare.sh",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "author": "admin@persistence.one",
  "license": "ASLv2",
  "bugs": {
    "url": "https://github.com/persistenceOne/persistenceJS/issues"
  },
  "homepage": "https://github.com/persistenceOne/persistenceJS#readme",
  "dependencies": {
    "@cosmjs/amino": "^0.28.11",
    "@cosmjs/cosmwasm-stargate": "^0.28.11",
    "@cosmjs/crypto": "^0.28.11",
    "@cosmjs/ledger-amino": "^0.28.13",
    "@cosmjs/proto-signing": "^0.28.11",
    "@cosmjs/stargate": "^0.28.11",
    "@cosmjs/tendermint-rpc": "^0.28.11",
    "@keplr-wallet/types": "^0.10.23",
    "@osmonauts/ast": "^0.8.0",
    "@osmonauts/helpers": "^0.4.0",
    "@osmonauts/lcd": "^0.3.10",
    "@pyramation/protobufjs": "^6.10.5",
    "@tendermint/amino-js": "^0.6.2",
    "@tendermint/belt": "^0.3.0",
    "@tendermint/sig": "^0.6.0",
    "bip32": "^2.0.6",
    "bip39": "^3.0.2",
    "buffer": "^5.6.0",
    "crypto": "^1.0.1",
    "es6-promise": "^4.2.8",
    "pako": "^2.0.4",
    "promise": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/generator": "7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/parser": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/traverse": "7.18.10",
    "@babel/types": "7.18.10",
    "@cosmwasm/ts-codegen": "^0.14.0",
    "@osmonauts/telescope": "^0.10.1",
    "@tanstack/react-query": "^4.2.3",
    "@types/node": "^18.6.3",
    "@types/node-fetch": "^2.6.2",
    "@types/pako": "^2.0.0",
    "@types/request": "^2.48.5",
    "husky": "^8.0.1",
    "json-schema-to-typescript": "^11.0.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "ts-proto": "^1.116.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}
